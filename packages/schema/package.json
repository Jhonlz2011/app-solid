{
    "name": "@app/schema",
    "version": "1.0.0",
    "type": "module",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./tables": "./src/tables/index.ts",
        "./backend": "./src/backend.ts",
        "./frontend": "./src/frontend.ts",
        "./types": "./src/types.ts",
        "./enums": "./src/enums.ts",
        "./ws-events": "./src/ws-events.ts"
    },
    "typesVersions": {
        "*": {
            "tables": [
                "./src/tables/index.ts"
            ],
            "frontend": [
                "./src/frontend.ts"
            ],
            "backend": [
                "./src/backend.ts"
            ],
            "types": [
                "./src/types.ts"
            ],
            "enums": [
                "./src/enums.ts"
            ],
            "ws-events": [
                "./src/ws-events.ts"
            ]
        }
    },
    "dependencies": {
        "drizzle-orm": "^0.44.7",
        "drizzle-typebox": "^0.3.3",
        "postgres": "^3.4.7",
        "valibot": "^1.2.0"
    },
    "peerDependencies": {
        "@sinclair/typebox": "^0.34.48"
    },
    "devDependencies": {
        "drizzle-valibot": "^0.4.2",
        "typescript": "^5.7.2"
    }
}